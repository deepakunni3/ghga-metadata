            "items": {
                "$ref": "src/content_schemas/Attribute"
            },
            "type": "array"
        },
        "cell_barcode_offset": {
            "description": "The offset in sequence of the cell identifying barcode (e.g., 0).",
            "type": "string"
        },
        "cell_barcode_read": {
            "description": "Permitted Values to indicate the location of a sequence component in a read or index",
            "enum": [
                "INDEX1",
                "INDEX2",
                "READ1",
                "READ2"
            ],
            "title": "IndexReadEnum",
            "type": "string",
            "$id": "src/content_schemas/IndexReadEnum"
        },
        "cell_barcode_size": {
