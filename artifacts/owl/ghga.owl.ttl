            owl:onProperty GHGA:concept_identifier ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty GHGA:ontology_name ],
        GHGA:OntologyClassMixin,
        GHGA:Population ;
    skos:altLabel "ancestral group",
        "ancestry category" ;
    skos:definition "Population category defined using ancestry informative markers (AIMs) based on genetic/genomic data." ;
