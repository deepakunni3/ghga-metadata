{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "additionalProperties": false,
    "description": "A file is an object that contains information generated from a process, either an Experiment or an Analysis.",
    "properties": {
        "title": {
            "type": "string",
            "description": "A comprehensive title for this Study."
        },
        "description": {
            "type": "string",
            "description": "A detailed description (abstract) that describes the goals of this Study."
        },
        "type": {
            "type": "string",
            "enum": [
                "CANCER_GENOMICS",
                "EPIGENETICS",
                "EXOME_SEQUENCING",
                "FORENSIC_GENETICS",
                "PALEO_GENOMICS",
                "GENE_REGULATION_STUDY",
                "METAGENOMICS",
                "POOLED_CLONE_SEQUENCING",
                "POPULATION_GENOMICS",
                "RNASEQ",
                "RESEQUENCING",
                "SYNTHETIC_GENOMICS",
                "TRANSCRIPTOME_ANALYSIS",
                "WHOLE_GENOME_SEQUENCING",
                "GWAS",
                "OTHER"
            ],
            "description": "The type of this Study (e.g., Cancer Genomics, Epigenetics, Exome Sequencing)."
        },
        "affiliations": {
            "type": "string",
            "description": "The affiliations associated with this Study."
        },
        "attributes": {
            "type": "object",
            "properties": {
                "key": {
                    "type": "string",
                    "description": ""
                },
                "value": {
                    "type": "string",
                    "description": ""
                }
            },
            "required": [
                "key",
                "value"
            ],
            "description": "One or more attributes that further characterize this Study."
        }
    },
    "required": [
        "filename",
        "format",
        "checksum",
        "size"
    ],
    "type": "object"
}